//output: 120
int main(){
    int sum=0;
    int p[3][4][5];
    for(int i=0;i<3;i++){
        for(int j=0;j<4;j++){
            for(int k=0;k<5;k++){
                p[i][j][k]=1;
            }
        }
    }
    for(int a=0;a<3;a++){
        for(int b=0;b<4;b++){
            for(int c=0;c<5;c++){
                sum+=p[a][b][c]*2;
            }
        }
    }
    write(sum);
    return 0;
}