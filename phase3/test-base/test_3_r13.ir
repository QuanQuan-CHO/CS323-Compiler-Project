FUNCTION factorial :
PARAM n
t11 := n
t12 := #0
IF t11 == t12 GOTO label1
GOTO label2
LABEL label1 :
t10 := #1
RETURN t10
GOTO label3
LABEL label2 :
t5 := n
t8 := n
t9 := #1
t7 := t8 - t9
ARG t7
t6 := CALL factorial
t4 := t5 * t6
RETURN t4
LABEL label3 :

FUNCTION main :
READ num
t3 := num
ARG t3
result := CALL factorial
t2 := result
WRITE t2
t1 := #0
RETURN t1
