FUNCTION mod :
PARAM x
PARAM n
t23 := x
t27 := x
t28 := n
t25 := t27 / t28
t26 := n
t24 := t25 * t26
t22 := t23 - t24
RETURN t22

FUNCTION isPalindrome :
PARAM num
reversed := #0
original := num
digit := #0
LABEL label7 :
t20 := num
t21 := #0
IF t20 != t21 GOTO label8
GOTO label9
LABEL label8 :
t18 := num
t19 := #10
ARG t19
ARG t18
digit := CALL mod
t16 := reversed
t17 := #10
t14 := t16 * t17
t15 := digit
reversed := t14 + t15
t12 := num
t13 := #10
num := t12 / t13
GOTO label7
LABEL label9 :
t10 := reversed
t11 := original
IF t10 == t11 GOTO label4
GOTO label5
LABEL label4 :
t9 := #1
RETURN t9
GOTO label6
LABEL label5 :
t8 := #0
RETURN t8
LABEL label6 :

FUNCTION main :
