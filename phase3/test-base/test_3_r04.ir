FUNCTION mod :
PARAM x
PARAM n
t27 := x
t31 := x
t32 := n
t29 := t31 / t32
t30 := n
t28 := t29 * t30
t26 := t27 - t28
RETURN t26

FUNCTION isPerfectNumber :
PARAM number
sum := #0
j := #1
LABEL label9 :
t22 := j
t24 := number
t25 := #2
t23 := t24 / t25
IF t22 <= t23 GOTO label10
GOTO label11
LABEL label10 :
t20 := number
t21 := j
ARG t21
ARG t20
t18 := CALL mod
t19 := #0
IF t18 == t19 GOTO label12
GOTO label13
LABEL label12 :
t16 := sum
t17 := j
sum := t16 + t17
LABEL label13 :
t14 := j
t15 := #1
j := t14 + t15
GOTO label9
LABEL label11 :
t12 := sum
t13 := number
IF t12 == t13 GOTO label6
GOTO label7
LABEL label6 :
t11 := #1
RETURN t11
GOTO label8
LABEL label7 :
t10 := #0
RETURN t10
LABEL label8 :

FUNCTION main :
count := #0
