FUNCTION mod :
PARAM x
PARAM n
t2 := x
t6 := x
t7 := n
t4 := t6 / t7
t5 := n
t3 := t4 * t5
t1 := t2 - t3
RETURN t1

FUNCTION isPalindrome :
PARAM num
reversed := #0
original := num
digit := #0
LABEL label1 :
t8 := num
t9 := #0
IF t8 != t9 GOTO label2
GOTO label3
LABEL label2 :
t10 := num
t11 := #10
ARG t11
ARG t10
digit := CALL mod
t14 := reversed
t15 := #10
t12 := t14 * t15
t13 := digit
reversed := t12 + t13
t16 := num
t17 := #10
num := t16 / t17
GOTO label1
LABEL label3 :
t18 := reversed
t19 := original
IF t18 == t19 GOTO label4
GOTO label5
LABEL label4 :
t20 := #1
RETURN t20
GOTO label6
LABEL label5 :
t21 := #0
RETURN t21
LABEL label6 :

FUNCTION main :
READ number
t24 := number
ARG t24
