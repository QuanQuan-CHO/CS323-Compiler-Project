FUNCTION factorial :
PARAM n
t1 := n
t2 := #0
IF t1 == t2 GOTO label1
GOTO label2
LABEL label1 :
t3 := #1
RETURN t3
GOTO label3
LABEL label2 :
t5 := n
t8 := n
t9 := #1
t7 := t8 - t9
ARG t7
t6 := CALL factorial
t4 := t5 * t6
RETURN t4
LABEL label3 :

FUNCTION main :
READ num
t10 := num
ARG t10
result := CALL factorial
t11 := result
WRITE t11
t12 := #0
RETURN t12
