FUNCTION mod :
PARAM x
PARAM n
t2 := x
t6 := x
t7 := n
t4 := t6 / t7
t5 := n
t3 := t4 * t5
t1 := t2 - t3
RETURN t1

FUNCTION isPerfectNumber :
PARAM number
sum := #0
j := #1
LABEL label1 :
t8 := j
t10 := number
t11 := #2
t9 := t10 / t11
IF t8 <= t9 GOTO label2
GOTO label3
LABEL label2 :
t14 := number
t15 := j
ARG t15
ARG t14
t12 := CALL mod
t13 := #0
IF t12 == t13 GOTO label4
GOTO label5
LABEL label4 :
t16 := sum
t17 := j
sum := t16 + t17
LABEL label5 :
t18 := j
t19 := #1
j := t18 + t19
GOTO label1
LABEL label3 :
t20 := sum
t21 := number
IF t20 == t21 GOTO label6
GOTO label7
LABEL label6 :
t22 := #1
RETURN t22
GOTO label8
LABEL label7 :
t23 := #0
RETURN t23
LABEL label8 :

FUNCTION main :
count := #0
i := #1
LABEL label9 :
t24 := i
t25 := #100
IF t24 <= t25 GOTO label10
GOTO label11
LABEL label10 :
t28 := i
ARG t28
t26 := CALL isPerfectNumber
t27 := #1
IF t26 == t27 GOTO label12
GOTO label13
LABEL label12 :
t29 := i
WRITE t29
LABEL label13 :
t30 := i
t31 := #1
i := t30 + t31
GOTO label9
LABEL label11 :
t32 := #0
RETURN t32
