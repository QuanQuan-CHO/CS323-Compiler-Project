FUNCTION self_plus :
a := #0 - #1
a := a + #1
WRITE a
RETURN #0

FUNCTION self_minus :
b := #1
b := b - #1
WRITE b
RETURN #0

FUNCTION plus_assign :
c := #0 - #6
c := c + #6
WRITE c
RETURN #0

FUNCTION minus_assign :
d := #7
d := d - #7
WRITE d
RETURN #0

FUNCTION mul_assign :
e := #3
e := e * #4
WRITE e
RETURN #0

FUNCTION div_assign :
f := #15
f := f / #5
WRITE f
RETURN #0

FUNCTION main :
$27 := CALL self_plus
$28 := CALL self_minus
$29 := CALL plus_assign
$30 := CALL minus_assign
$31 := CALL mul_assign
$32 := CALL div_assign
RETURN #0
